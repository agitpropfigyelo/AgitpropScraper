{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Aspire.Hosting.Dcp": "Warning",
      "Agitprop.Infrastructure": "Trace",
      "Agitprop.Scraper.Consumer": "Trace"
    }
  },
  "ConcurrencyLimit": 4,
  "AllowedHosts": "*",
  "Retry": {
    "Spider": 3,
    "PageLoader": 3,
    "ProxyProvider": 3,
    "SurrealDB": 2,
    "NewsfeedSink": 2,
    "NLPService": 2,
    "PuppeteerPageLoader": 2,
    "PuppeteerPageLoaderWithProxies": 2
  },
    "Proxy": {
    "ValidateEndpoint": "https://www.vanenet.hu/",
    "ValidationTimeoutSeconds": 10,
    "ValidationParallelism": 10,
    "MinAliveProxies": 2,
    "TargetAliveProxies": 5,
    "MaxCachedProxies": 2000
  },
  "HttpClientDefaults": {
    "MaxConnectionsPerServer": 100,
    "PooledConnectionLifetimeSeconds": 30,
    "PooledConnectionIdleTimeoutSeconds": 30
  },
  "Metrics": {
    "Enabled": true,
    "IncludeSpiderMetrics": true,
    "HistogramBuckets": {
      "PageLoadTime": [100, 500, 1000, 2000, 5000, 10000, 30000, 60000],
      "ProcessingTime": [500, 1000, 2000, 5000, 10000, 20000, 60000]
    }
  }
}

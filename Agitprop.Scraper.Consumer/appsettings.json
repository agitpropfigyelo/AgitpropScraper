{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "ConcurrencyLimit": 8,
  "AllowedHosts": "*",
  "Retry": {
    "Spider": 3,
    "PageLoader": 3,
    "ProxyProvider": 3,
    "SurrealDB": 3,
    "NewsfeedSink": 3,
    "NLPService": 3,
    "PuppeteerPageLoader": 3,
    "PuppeteerPageLoaderWithProxies": 3
  },
    "Proxy": {
    "SourceUrl": "https://cdn.jsdelivr.net/gh/proxifly/free-proxy-list@main/proxies/protocols/https/data.json",
    "ValidateEndpoint": "https://www.vanenet.hu/",
    "ValidationTimeoutSeconds": 3,
    "ValidationParallelism": 50,
    "ValidationIntervalMinutes": 3,
    "MinAliveProxies": 10,
    "MaxCachedProxies": 2000
  },
  "HttpClientDefaults": {
    "MaxConnectionsPerServer": 100,
    "PooledConnectionLifetimeSeconds": 30,
    "PooledConnectionIdleTimeoutSeconds": 30,
    "RequestTimeoutSeconds": 15
  }
}

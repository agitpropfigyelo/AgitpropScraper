{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Agitprop.Infrastructure": "Debug"
    }
  },
  "Proxy": {
    "ValidateEndpoint": "https://www.vanenet.hu/",
    "ValidationTimeoutSeconds": 10,
    "ValidationParallelism": 50,
    "MinAliveProxies": 10,
    "TargetAliveProxies": 35,
    "MaxCachedProxies": 200
  },
  "HttpClientDefaults": {
    "MaxConnectionsPerServer": 100,
    "PooledConnectionLifetimeSeconds": 30,
    "PooledConnectionIdleTimeoutSeconds": 30
  }
}
